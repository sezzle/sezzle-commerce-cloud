<?xml version="1.0" encoding="UTF-8"?>
<metadata xmlns="http://www.demandware.com/xml/impex/metadata/2006-10-31">
    <type-extension type-id="Profile">
        <custom-attribute-definitions>
            <attribute-definition attribute-id="SezzleToken">
                <display-name xml:lang="x-default">Sezzle Token</display-name>
                <type>string</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <min-length>0</min-length>
            </attribute-definition>
            <attribute-definition attribute-id="SezzleTokenApproved">
                <display-name xml:lang="x-default">Sezzle Token Approved</display-name>
                <type>boolean</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <min-length>0</min-length>
            </attribute-definition>
            <attribute-definition attribute-id="SezzleTokenExpiration">
                <display-name xml:lang="x-default">Sezzle Token Expiration</display-name>
                <type>string</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <min-length>0</min-length>
            </attribute-definition>
        </custom-attribute-definitions>
        <group-definitions>
            <attribute-group group-id="Sezzle">
                <display-name xml:lang="x-default">Sezzle</display-name>
                <attribute attribute-id="SezzleToken"/>
                <attribute attribute-id="SezzleTokenApproved"/>
                <attribute attribute-id="SezzleTokenExpiration"/>
            </attribute-group>
        </group-definitions>
    </type-extension>
    <type-extension type-id="Order">
        <custom-attribute-definitions>
            <attribute-definition attribute-id="SezzleExternalId">
                <display-name xml:lang="x-default">Sezzle External Id</display-name>
                <description xml:lang="x-default">Sezzle Charge ID</description>
                <type>string</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>true</externally-managed-flag>
                <min-length>0</min-length>
            </attribute-definition>
            <attribute-definition attribute-id="SezzleOrderUUID">
                <display-name xml:lang="x-default">Sezzle Order UUID</display-name>
                <description xml:lang="x-default">Sezzle Order UUID</description>
                <type>string</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>true</externally-managed-flag>
                <min-length>0</min-length>
            </attribute-definition>
            <attribute-definition attribute-id="SezzleAuthExpiration">
                <display-name xml:lang="x-default">Sezzle Auth Expiration</display-name>
                <description xml:lang="x-default">Sezzle Auth Expiration</description>
                <type>string</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>true</externally-managed-flag>
                <min-length>0</min-length>
            </attribute-definition>
            <attribute-definition attribute-id="SezzleOrderAmount">
                <display-name xml:lang="x-default">Sezzle Order Amount</display-name>
                <description xml:lang="x-default">Order Amount in Cents</description>
                <type>string</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>true</externally-managed-flag>
                <min-length>0</min-length>
            </attribute-definition>
            <attribute-definition attribute-id="SezzleCapturedAmount">
                <display-name xml:lang="x-default">Sezzle Captured Amount</display-name>
                <description xml:lang="x-default">Captured Amount in Cents</description>
                <type>string</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>true</externally-managed-flag>
                <min-length>0</min-length>
            </attribute-definition>
            <attribute-definition attribute-id="SezzleRefundedAmount">
                <display-name xml:lang="x-default">Sezzle Refunded Amount</display-name>
                <description xml:lang="x-default">Refunded Amount in Cents</description>
                <type>string</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>true</externally-managed-flag>
                <min-length>0</min-length>
            </attribute-definition>
            <attribute-definition attribute-id="SezzleReleasedAmount">
                <display-name xml:lang="x-default">Sezzle Released Amount</display-name>
                <description xml:lang="x-default">Released Amount in Cents</description>
                <type>string</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>true</externally-managed-flag>
                <min-length>0</min-length>
            </attribute-definition>
            <attribute-definition attribute-id="SezzlePaymentAction">
                <display-name xml:lang="x-default">Sezzle Payment Action</display-name>
                <description xml:lang="x-default">Defines by what action order was placed</description>
                <type>enum-of-string</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>true</externally-managed-flag>
                <value-definitions>
                    <value-definition>
                        <display xml:lang="x-default">AUTHORIZATION</display>
                        <value>AUTH</value>
                    </value-definition>
                    <value-definition>
                        <display xml:lang="x-default">AUTHORIZATION + CAPTURE</display>
                        <value>CAPTURE</value>
                    </value-definition>
                </value-definitions>
            </attribute-definition>
            <attribute-definition attribute-id="SezzleToken">
                <display-name xml:lang="x-default">Sezzle Token</display-name>
                <description xml:lang="x-default">Sezzle Tokenized token</description>
                <type>string</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>true</externally-managed-flag>
                <min-length>0</min-length>
            </attribute-definition>
            <attribute-definition attribute-id="SezzleTokenExpiration">
                <display-name xml:lang="x-default">Sezzle Token Expiration</display-name>
                <description xml:lang="x-default">Sezzle Token Expiration</description>
                <type>string</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>true</externally-managed-flag>
                <min-length>0</min-length>
            </attribute-definition>
