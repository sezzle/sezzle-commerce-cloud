<?xml version="1.0" encoding="UTF-8"?>
<services xmlns="http://www.demandware.com/xml/impex/services/2014-09-26">
    <service-profile service-profile-id="Sezzle">
        <timeout-millis>60000</timeout-millis>
        <rate-limit-enabled>true</rate-limit-enabled>
        <rate-limit-calls>5</rate-limit-calls>
        <rate-limit-millis>50000</rate-limit-millis>
        <cb-enabled>true</cb-enabled>
        <cb-calls>3</cb-calls>
        <cb-millis>60000</cb-millis>
    </service-profile>

    <service service-id="sezzle.authenticate">
        <service-type>HTTP</service-type>
        <enabled>true</enabled>
        <log-prefix>sezzle</log-prefix>
        <comm-log-enabled>false</comm-log-enabled>
        <mock-mode-enabled>false</mock-mode-enabled>
        <profile-id>Sezzle</profile-id>
        <credential-id/>
    </service>

    <service service-id="sezzle.auth">
        <service-type>HTTP</service-type>
        <enabled>true</enabled>
        <log-prefix>sezzle</log-prefix>
        <comm-log-enabled>false</comm-log-enabled>
        <mock-mode-enabled>false</mock-mode-enabled>
        <profile-id>Sezzle</profile-id>
        <credential-id/>
    </service>

    <service service-id="sezzle.capture">
